{
  "name": "webrtc-practice",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=16.20.2"
  },
  "scripts": {
    "serve": "nodemon",
    "start:dev": "node --require ts-node/register --require tsconfig-paths/register --experimental-modules src/server.ts",
    "tsc:watch": "tsc --watch",
    "sass": "node-sass src/styles/sass/ --output dist/styles/css/ --indent-width 2 --output-style expanded --indent-type space --source-map true",
    "sass:watch": "yarn run sass -- --watch"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.8",
    "concurrently": "^8.2.2",
    "node-sass": "^9.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
